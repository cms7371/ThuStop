<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="AgreeTermsfrag"
            type="com.thustop_00.RegisterTermsFragment" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="?attr/actionBarSize" />

        <com.thustop_00.widgets.NotoTextView
            android:id="@+id/tv_agree"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/tv_agree"
            android:textColor="#202020"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@id/iv_agree"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/guideline1"
            app:layout_constraintVertical_bias="0.110000014"
            app:layout_constraintVertical_chainStyle="packed"
            app:notoTextStyle="medium" />

        <ImageView
            android:id="@+id/iv_agree"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="21dp"
            app:srcCompat="@drawable/img_terms"
            app:layout_constraintTop_toBottomOf="@id/tv_agree"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/cb_agree_all"
            />

        <CheckBox
            android:id="@+id/cb_agree_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:button="@drawable/checkbox_round"
            android:paddingStart="10dp"
            app:layout_constraintTop_toBottomOf="@id/iv_agree"
            app:layout_constraintStart_toStartOf="@id/tv_agree"
            app:layout_constraintBottom_toTopOf="@id/cb_agree1"
            app:layout_constraintVertical_chainStyle="packed"/>

        <com.thustop_00.widgets.NotoTextView
            android:id="@+id/tv_agree_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cb_agree_all"
            android:textColor="#202020"
            android:textSize="14sp"
            app:notoTextStyle="regular"

            app:layout_constraintStart_toEndOf="@id/cb_agree_all"
            app:layout_constraintTop_toTopOf="@id/cb_agree_all"
            app:layout_constraintBottom_toBottomOf="@id/cb_agree_all"/>


        <CheckBox
            android:id="@+id/cb_agree1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:button="@drawable/checkbox_trans_sel"
            android:paddingStart="10dp"
            app:layout_constraintTop_toBottomOf="@id/cb_agree_all"
            app:layout_constraintStart_toStartOf="@id/tv_agree"
            app:layout_constraintBottom_toTopOf="@id/cb_agree2"/>


        <com.thustop_00.widgets.NotoTextView
            android:id="@+id/tv_agree1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cb_agree1"
            android:textColor="#4b4b4b"
            android:textSize="12sp"
            app:notoTextStyle="regular"
            app:layout_constraintStart_toEndOf="@id/cb_agree1"
            app:layout_constraintTop_toTopOf="@id/cb_agree1"
            app:layout_constraintBottom_toBottomOf="@id/cb_agree1"/>

        <CheckBox
            android:id="@+id/cb_agree2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:button="@drawable/checkbox_trans_sel"
            android:paddingStart="10dp"
            app:layout_constraintTop_toBottomOf="@id/cb_agree1"
            app:layout_constraintStart_toStartOf="@id/tv_agree"
            app:layout_constraintBottom_toTopOf="@id/cb_agree3"/>

        <com.thustop_00.widgets.NotoTextView
            android:id="@+id/tv_agree2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cb_agree2"
            android:textColor="#4b4b4b"
            android:textSize="12sp"
            app:notoTextStyle="regular"
            app:layout_constraintStart_toEndOf="@id/cb_agree2"
            app:layout_constraintTop_toTopOf="@id/cb_agree2"
            app:layout_constraintBottom_toBottomOf="@id/cb_agree2"/>

        <CheckBox
            android:id="@+id/cb_agree3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:button="@drawable/checkbox_trans_sel"
            android:paddingStart="10dp"
            app:layout_constraintTop_toBottomOf="@id/cb_agree2"
            app:layout_constraintStart_toStartOf="@id/tv_agree"
            app:layout_constraintBottom_toTopOf="@id/cb_agree4"/>

        <com.thustop_00.widgets.NotoTextView
            android:id="@+id/tv_agree3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cb_agree3"
            android:textColor="#4b4b4b"
            android:textSize="12sp"
            app:notoTextStyle="regular"
            app:layout_constraintStart_toEndOf="@id/cb_agree3"
            app:layout_constraintTop_toTopOf="@id/cb_agree3"
            app:layout_constraintBottom_toBottomOf="@id/cb_agree3"/>

        <CheckBox
            android:id="@+id/cb_agree4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:button="@drawable/checkbox_trans_sel"
            android:paddingStart="10dp"
            app:layout_constraintTop_toBottomOf="@id/cb_agree3"
            app:layout_constraintStart_toStartOf="@id/tv_agree"
            app:layout_constraintBottom_toTopOf="@id/cb_agree5"/>
        <com.thustop_00.widgets.NotoTextView
            android:id="@+id/tv_agree4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cb_agree4"
            android:textColor="#4b4b4b"
            android:textSize="12sp"
            app:notoTextStyle="regular"
            app:layout_constraintStart_toEndOf="@id/cb_agree4"
            app:layout_constraintTop_toTopOf="@id/cb_agree4"
            app:layout_constraintBottom_toBottomOf="@id/cb_agree4"/>

        <CheckBox
            android:id="@+id/cb_agree5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:button="@drawable/checkbox_trans_sel"
            android:paddingStart="10dp"
            app:layout_constraintTop_toBottomOf="@id/cb_agree4"
            app:layout_constraintStart_toStartOf="@id/tv_agree"
            app:layout_constraintBottom_toTopOf="@id/cb_agree6"/>

        <com.thustop_00.widgets.NotoTextView
            android:id="@+id/tv_agree5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cb_agree5"
            android:textColor="#4b4b4b"
            android:textSize="12sp"
            app:notoTextStyle="regular"
            app:layout_constraintStart_toEndOf="@id/cb_agree5"
            app:layout_constraintTop_toTopOf="@id/cb_agree5"
            app:layout_constraintBottom_toBottomOf="@id/cb_agree5"/>

        <CheckBox
            android:id="@+id/cb_agree6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:button="@drawable/checkbox_trans_sel"
            android:paddingStart="10dp"
            app:layout_constraintTop_toBottomOf="@id/cb_agree5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@id/tv_agree"/>

        <com.thustop_00.widgets.NotoTextView
            android:id="@+id/tv_agree6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cb_agree6"
            android:textColor="#4b4b4b"
            android:textSize="12sp"
            app:notoTextStyle="regular"
            app:layout_constraintStart_toEndOf="@id/cb_agree6"
            app:layout_constraintTop_toTopOf="@id/cb_agree6"
            app:layout_constraintBottom_toBottomOf="@id/cb_agree6"/>

        <com.thustop_00.widgets.NotoButton
            android:id="@+id/bt_agree"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintWidth_percent="0.75"
            android:layout_marginBottom="50dp"
            android:background="@drawable/bg_round25_grayc4"
            android:text="@string/bt_agree"
            android:textColor="#ffffff"
            app:notoTextStyle="medium"
            android:onClick="@{AgreeTermsfrag::onAgreeClick}"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>